library(surreal)
# 将信息转化为二维格点形式的图片并隐藏进数据集中
message_data <- surreal_text("I\nknow\nyour secret!")
# 通过线性回归恢复隐藏信息
model <- lm(y ~ ., data = message_data)
plot(model$fitted, model$resid, pch = 16,
main = "Custom Message in Residuals")
